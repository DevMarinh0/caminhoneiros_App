generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cadastro {
  id             Int      @id @default(autoincrement())
  transportadora String
  nome           String
  placa          String
  destino        String
  dataCadastro   String
  fotos          Foto[]
}

model Foto {
  id         Int      @id @default(autoincrement())
  url        String
  cadastro   Cadastro @relation(fields: [cadastroId], references: [id], onDelete: Cascade)
  cadastroId Int
}